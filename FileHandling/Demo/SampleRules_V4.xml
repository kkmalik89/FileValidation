<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE RULES SYSTEM "SampleRules_V4.dtd" >
<RULES>
	 <FILE_DEFINITION_RULES>
			<FILE_TYPE>DELIMITED</FILE_TYPE>
			<DELIMITER>,</DELIMITER>
			<FILE_HEADER_AVAILABLE>Y</FILE_HEADER_AVAILABLE>
			<FILE_HEADER>EMP_ID,EMP_NAME,SALARY,JOINING_DATE,OFFICE_LOCATION,MOBILE_NUMBER</FILE_HEADER>
			<COLUMN_LIST>
				<COLUMN>
					<NAME>EMP_ID</NAME>
					<EXPECTED_DATA_TYPE>STRING</EXPECTED_DATA_TYPE>
					<MIN_LENGTH></MIN_LENGTH>
					<MAX_LENGTH></MAX_LENGTH>
					<NULLABLE>N</NULLABLE>
					<ALLOWED_LOV></ALLOWED_LOV>
					<IS_PK>Y</IS_PK>
				</COLUMN>
				<COLUMN>
					<NAME>EMP_NAME</NAME>
					<EXPECTED_DATA_TYPE>STRING</EXPECTED_DATA_TYPE>
					<MIN_LENGTH>1</MIN_LENGTH>
					<MAX_LENGTH>255</MAX_LENGTH>
					<NULLABLE>N</NULLABLE>
					<ALLOWED_LOV></ALLOWED_LOV>
					<IS_PK>N</IS_PK>
				</COLUMN>
				<COLUMN>
					<NAME>SALARY</NAME>
					<EXPECTED_DATA_TYPE>NUMERIC</EXPECTED_DATA_TYPE>
					<EXPECTED_PRECISION></EXPECTED_PRECISION>
					<EXPECTED_SCALE>2</EXPECTED_SCALE>
					<NULLABLE></NULLABLE>
					<ALLOWED_LOV></ALLOWED_LOV>
					<IS_PK></IS_PK>
				</COLUMN>
				<COLUMN>
					<NAME>JOINING_DATE</NAME>
					<EXPECTED_DATA_TYPE>DATE</EXPECTED_DATA_TYPE>
					<EXPECTED_FORMAT>MM/dd/yyyy</EXPECTED_FORMAT>
					<NULLABLE></NULLABLE>
					<ALLOWED_LOV></ALLOWED_LOV>
					<IS_PK></IS_PK>
				</COLUMN>
				<COLUMN>
					<NAME>MOBILE_NUMBER</NAME>
					<EXPECTED_DATA_TYPE>STRING</EXPECTED_DATA_TYPE>
					<MIN_LENGTH>10</MIN_LENGTH>
					<MAX_LENGTH>10</MAX_LENGTH>
					<NULLABLE>Y</NULLABLE>
					<ALLOWED_LOV></ALLOWED_LOV>
					<IS_PK>N</IS_PK>
				</COLUMN>
				<COLUMN>
					<NAME>OFFICE_LOCATION</NAME>
					<EXPECTED_DATA_TYPE>STRING</EXPECTED_DATA_TYPE>
					<MIN_LENGTH>1</MIN_LENGTH>
					<MAX_LENGTH>20</MAX_LENGTH>
					<NULLABLE>Y</NULLABLE>
					<ALLOWED_LOV>
						<VALUE>MUMBAI</VALUE>
						<VALUE>BANGALORE</VALUE>
						<VALUE>HYDERABAD</VALUE>
						<VALUE>DELHI</VALUE>
					</ALLOWED_LOV>
					<IS_PK>N</IS_PK>
				</COLUMN>
			</COLUMN_LIST>
	 </FILE_DEFINITION_RULES>
</RULES>
